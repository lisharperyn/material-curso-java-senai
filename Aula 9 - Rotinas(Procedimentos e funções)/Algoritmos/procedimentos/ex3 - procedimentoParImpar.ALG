algoritmo "semnome"
var
  numero: inteiro
  
  procedimento parImpar(numero: inteiro)
  inicio
     se(numero % 2 = 0) entao
       escreval("O numero ",numero," é par")
     senao
       escreval("O numero ",numero," é impar")
     fimse
  
  fimprocedimento
inicio
    
    parImpar(4)
    parImpar(3)
    parimpar(50)
    
    
fimalgoritmo