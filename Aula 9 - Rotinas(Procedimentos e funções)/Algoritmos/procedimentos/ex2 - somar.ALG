algoritmo "semnome"
var
  numero,numero2,soma: inteiro
  
  procedimento somarNumeros(n1,n2: inteiro)
  inicio
     escreval("A soma entre os valores é:", n1 + n2)
  fimprocedimento
inicio

   numero<- 5
   numero2<- 6
   
   somarNumeros(numero,numero2)
   somarNumeros(5,8)
   somarNumeros(numero, 9)
   somarNumeros(15, numero2)

fimalgoritmo