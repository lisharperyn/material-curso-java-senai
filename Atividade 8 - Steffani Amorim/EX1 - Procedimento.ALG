algoritmo "Ex1 - Procedimento"
//Pedir para o usuário digitar um número e imprimir este número na tela.

var

   numero:  inteiro
   
   procedimento informeNumero()
   inicio
   escreval("Insira um número: ")
   leia(numero)
   escreval()
   escreval("Você inseriu o número ", numero)
   fimprocedimento
   
   

inicio

    informeNumero()

fimalgoritmo