algoritmo "Ex1 - Funcao"
//Pedir para o usuário digitar um número e imprimir este número na tela.
var
   numero: inteiro
   imprimaNumero: inteiro

funcao informeNumero(n:inteiro): inteiro
inicio
retorne n
fimfuncao

inicio
   
   escreval("Insira um número: ")
   leia(numero)
   imprimaNumero <- informeNumero(numero)

   escreva(imprimaNumero)

fimalgoritmo