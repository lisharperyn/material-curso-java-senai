algoritmo "semnome"
var
   nome: caractere
   idade:inteiro
   salario: real
inicio
    escreva("Nome: ")
    leia(nome)
    
    escreva("Idade: ")
    leia(idade)
    
    escreva("Salario: ")
    leia(salario)
    
    escreval("Nome: ", nome)
    escreval("Idade: ", idade)
    escreval("Salario: ", salario)
fimalgoritmo